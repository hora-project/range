{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "globals": {
    "console": true
  },
  "rules": {
    "no-console": 0,
    "no-fallthrough": 0,
    "no-constant-condition": 0
  }
}
